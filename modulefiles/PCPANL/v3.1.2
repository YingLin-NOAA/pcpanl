proc ModulesHelp { } {
puts stderr "Set environment veriables for PCPANL"
puts stderr "This module initializes the environment "
puts stderr "for the Intel Compiler Suite $version\n"
}
module-whatis "PCPANL whatis description"

set ver v3.1.2
set envir prod

set sys [uname sysname]

setenv COMP ifort

setenv D_FFLAGS_CHKBNDS "-g -check bounds -traceback"

# Loading Intel Compiler Suite
module load ics/15.0.1

# Loading nceplibs modules
module load bacio/v2.0.2
module load w3nco/v2.0.6
module load w3emc/v2.2.0
module load bufr/v11.0.0

# NCO: please replace the above with the following: 
setenv WGRIB2LIB /nw${envir}2/pcpanl.${ver}/lib/grib2/lib/libwgrib2.a
setenv WGRIB2INC /nw${envir}2/pcpanl.${ver}/lib/grib2/lib
